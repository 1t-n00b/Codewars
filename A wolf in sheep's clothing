function warnTheSheep(queue) {
  for (let i=0; i < queue.length; i++) {

    if (queue[i]=="wolf" ) {
      
    let con = queue.length-i-1;
      if (con==0) {
        return "Pls go away and stop eating my sheep";
      }
      else {
        console.log("Oi! Sheep number " + con + "! You are about to be eaten by a wolf!");
    return "Oi! Sheep number " + con + "! You are about to be eaten by a wolf!"; 
      }
    
      }
    
    
   
  }
   

  }
